{
  "name": "@securelend/sdk",
  "version": "1.0.0",
  "description": "SecureLend SDK for JavaScript/TypeScript - Financial services infrastructure for AI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "securelend",
    "sdk",
    "loans",
    "banking",
    "credit-cards",
    "fintech",
    "financial-services",
    "ai"
  ],
  "author": "SecureLend <support@securelend.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/securelend/sdk.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/securelend/sdk/issues"
  },
  "homepage": "https://securelend.ai/docs/sdk",
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
